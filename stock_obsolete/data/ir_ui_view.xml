<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="ir_ui_view_844_custom_view" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <header position="after">
          <div class="alert alert-warning text-center o_form_header" attrs="{'invisible': [('x_obsolete_date','=',False)]}">
            <div class="text-left"><button icon="fa-hourglass-end" class="btn btn-link alert-warning"/><b>Obsolete Warning </b> - this product was marked obsolete effective <field name="x_obsolete_date" readonly="1"/> by <field name="x_obsolete_user" readonly="1"/>
    </div>
          </div>
        </header>
        <field name="categ_id" position="after">
          <field name="x_obsolete_date"/>
          <field name="x_obsolete_user" attrs="{'required': [('x_obsolete_date','!=',False)], 'invisible': [('x_obsolete_date','=',False)]}"/>
        </field>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="inherit_id" ref="product.product_template_form_view"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">product.template</field>
    <field name="name">stock_obsolete product template</field>
    <field name="priority">16</field>
    <field name="type">form</field>
  </record>
  <record id="ir_ui_view_845_custom_view" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <header position="after">
          <div class="alert alert-warning text-center o_form_header" attrs="{'invisible': [('x_contains_obsolete','=',False)]}">
            <div class="text-left"><field name="x_contains_obsolete" invisible="1"/><button icon="fa-hourglass-end" class="btn btn-link alert-warning"/><b>Obsolete Warning </b> - one or more products on this Transfer have been marked as obsolete
    </div>
          </div>
        </header>
        <xpath expr="//page[@name='operations']/field/tree/button[@name='action_assign_serial']" position="after">
          <field name="x_obsolete_date" invisible="1"/>
          <button icon="fa-hourglass-end" class="btn alert-warning" attrs="{'invisible': [('x_obsolete_date','=',False)]}" title="Obsolete Product"/>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="inherit_id" ref="stock.view_picking_form"/>
    <field name="key" eval="False"/>
    <field name="mode">extension</field>
    <field name="model">stock.picking</field>
    <field name="name">stock_obsolete stock picking</field>
    <field name="priority">16</field>
    <field name="type">form</field>
  </record>
