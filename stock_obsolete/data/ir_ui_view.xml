  <record id="ir_ui_view_877_custom_view" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <sheet position="before">
          <div class="alert alert-warning text-center o_form_header" attrs="{'invisible': [('x_contains_obsolete','=',False)]}">
            <div class="text-left"><field name="x_contains_obsolete" invisible="1"/><button icon="fa-hourglass-end" class="btn btn-link alert-warning"/><b>Obsolete Warning </b> - one or more products on this Bill of Materials have been marked as obsolete
      </div>
          </div>
        </sheet>
        <div class="oe_button_box" name="button_box" position="inside">
          <button string="Open Orders" type="action" name="%(ir_act_server_so_open_manufacturing_orders)d" class="oe_stat_button" icon="fa-wrench"/>
        </div>
        <xpath expr="//page[@name='components']/field/tree/field[@name='operation_id']" position="after">
          <field name="x_obsolete_date" invisible="1"/>
          <button icon="fa-hourglass-end" class="btn alert-warning" attrs="{'invisible': [('x_obsolete_date','=',False)]}" title="Obsolete Product"/>
