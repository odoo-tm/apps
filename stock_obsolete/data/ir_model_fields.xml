  <record id="ir_model_fields_4769_contains_obsolete" model="ir.model.fields" context="{'studio': True}">
    <field name="complete_name" eval="False"/>
    <field name="compute"><![CDATA[for record in self:
  record['x_contains_obsolete'] = False
  if record.state not in ['done','cancel']:
    for line in record.move_ids_without_package:
      if line.x_obsolete_date:
        record['x_contains_obsolete'] = True
]]></field>
    <field name="copied" eval="False"/>
    <field name="depends">move_ids_without_package.x_obsolete_date</field>
     <field name="field_description">Contains Obsolete Product</field>
    <field name="model">stock.picking</field>
    <field name="model_id" ref="stock.model_stock_picking"/>
    <field name="name">x_contains_obsolete</field>
    <field name="readonly" eval="True"/>
    <field name="state">manual</field>
    <field name="store" eval="True"/>
    <field name="ttype">boolean</field>
  </record>
